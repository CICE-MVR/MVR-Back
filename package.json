{
  "name": "quien-es-quien-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CICE-MVR/MVR-Back.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CICE-MVR/MVR-Back/issues"
  },
  "scripts": {
    "build": "babel src --out-dir ./dist --copy-files",
    "clean": "rm -rf ./dist",
    "start": "npm run build && node ./dist/index.js",
    "watch": "babel --watch src --out-dir ./dist --copy-files",
    "dev": "nodemon ./dist",
    "lint": "eslint ./src",
    "format": "prettier --write --ignore-path .gitignore .",
    "prepare": "husky install"
  },
  "homepage": "https://github.com/CICE-MVR/MVR-Back#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "connect-mongodb-session": "^2.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.7",
    "socket.io": "^4.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.27.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "prettier": "2.3.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "no-unused-vars": "error"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  }
}
